{
    "name": "Actinium",
    "version": "3.0.0-alpha",
    "author": "Cam Tullos",
    "scripts": {
        "start": "node ./src/index.js",
        "local": "nodemon",
        "docker": "npm-run-all -s docker:*",
        "docker:build": "docker image build -t atomicreactor/actinium:$npm_package_version .",
        "docker:push": "docker push atomicreactor/actinium:$npm_package_version",
        "docs": "apidoc -i src/app/cloud -o docs"
    },
    "dependencies": {
        "apidoc": "^0.17.7",
        "body-parser": "*",
        "chalk": "^2.3.0",
        "cookie-parser": "*",
        "cookie-session": "^1.3.2",
        "cors": "^2.8.1",
        "cross-env": "^5.2.0",
        "ejs": "*",
        "events": "^1.1.1",
        "express": "*",
        "fs-extra": "^3.0.1",
        "globby": "^8.0.1",
        "https": "^1.0.0",
        "javadoc": "^1.0.6",
        "js-beautify": "^1.8.1",
        "js-cookie": "^2.1.4",
        "jsonwebtoken": "^8.3.0",
        "kerberos": "0.0.24",
        "lodash": "^4.17.11",
        "moment": "*",
        "mongodb": "^2.2.36",
        "morgan": "^1.9.1",
        "nodemon": "^1.18.3",
        "npm-run-all": "^4.1.2",
        "object-path": "^0.11.4",
        "parse": "^1.11.1",
        "parse-dashboard": "^1.2.0",
        "parse-server": "^4.5.0",
        "pluralize": "^7.0.0",
        "promise": "^7.3.1",
        "request": "^2.88.0",
        "underscore": "^1.9.1"
    },
    "devDependencies": {
        "atomic-reactor-cli": "^2.1.1",
        "eslint": "^5.13.0",
        "husky": "^1.3.1",
        "lint-staged": "^8.1.5",
        "prettier": "^1.16.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Atomic-Reactor/Actinium-2.0"
    },
    "engines": {
        "node": "11.9.0",
        "npm": ">=6.5.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
